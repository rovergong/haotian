on init
    start vendor.keymint-qti

service vendor.keymint-qti /vendor/bin/hw/android.hardware.security.keymint-service-qti
    class early_hal
    user system
    group system drmrpc

on property:vendor.car.hiber=hiberExit
    stop vendor.keymint-qti
    start vendor.keymint-qti
    stop keystore2
    start keystore2
    seclabel u:r:recovery:s0

